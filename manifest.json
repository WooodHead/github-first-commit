{
"manifest_version": 2,
"name": "github first commit",
"version": "1.0.0",
"description": "Add 'First Page' and 'Last Page' buttons to Github's Commits page",

"background":{
"scripts":["background.js"]
},

"content_scripts": [
{
"js":["content.js"],
"matches":["https://github.com/*/*/commits/*"]
}
],

"browser_action": {
"default_icon":{"19":"images/icon19.png"}
},

"permissions":["tabs","activeTab", "*://github.com/*/*/commits/", "webNavigation"]

}